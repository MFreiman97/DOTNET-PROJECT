        <TextBox x:Name="StreetTextBox" HorizontalAlignment="Left" Height="21" Margin="201,221,0,0" TextWrapping="Wrap" Text="???" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="ApartmentTextBox" HorizontalAlignment="Left" Height="21" Margin="201,253,0,0" TextWrapping="Wrap" Text="???" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="63" Margin="139,290,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="254" BorderThickness="2"/>
        <ListBox x:Name="listBox" SelectionMode="Multiple" HorizontalAlignment="Left" Height="21" Margin="182,371,0,0" VerticalAlignment="Top" Width="139"/>
        <Label x:Name="label_Copy3" Content="days of working" HorizontalAlignment="Left" Margin="21,365,0,0" VerticalAlignment="Top" Width="113" Height="27"/>
        <TextBox x:Name="start1" HorizontalAlignment="Left" Height="20" Margin="21,404,0,0" TextWrapping="Wrap" Text="start" VerticalAlignment="Top" Width="85"/>
        <TextBox x:Name="end1" HorizontalAlignment="Left" Height="20" Margin="21,429,0,0" TextWrapping="Wrap" Text="end" VerticalAlignment="Top" Width="85"/>
        <TextBox x:Name="start2" HorizontalAlignment="Left" Height="20" Margin="111,404,0,0" TextWrapping="Wrap" Text="start" VerticalAlignment="Top" Width="85"/>
        <TextBox x:Name="end2" HorizontalAlignment="Left" Height="20" Margin="111,429,0,0" TextWrapping="Wrap" Text="end" VerticalAlignment="Top" Width="85"/>
